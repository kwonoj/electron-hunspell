{
  "compilerOptions": {
    "removeComments": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "noEmit": true,
    "lib": [
      "es5",
      "es2015",
      "dom"
    ],
  }
}